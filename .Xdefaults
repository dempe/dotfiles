URxvt.font: xft:Fira Code:regular:pixelsize=20:antialias=true:hinting=true,xft:Source Code Pro:medium:pixelsize=20:antialias=true:hinting=true,xft:Noto Mono:pixelsize=20:antialias=true:hinting=true,xft:Symbola
URxvt.boldFont: xft:Fira Code:bold:pixelsize=20:antialias=true:hinting=true,xft:Source Code Pro:semibold:pixelsize=20:antialias=true:hinting=true,xft:Noto Mono:pixelsize=20:antialias=true:hinting=true,xft:Symbola
URxvt.italicFont: xft:Fira Code:italic:pixelsize=20:antialias=true:hinting=true,xft:Source Code Pro:italic:pixelsize=20:antialias=true:hinting=true,xft:Noto Mono:pixelsize=20:antialias=true:hinting=true,xft:Symbola
URxvt.boldItalicFont: xft:Fira Code:bold:italic:pixelsize=20:antialias=true:hinting=true,xft:Source Code Pro:semibold:italic:pixelsize=20:antialias=true:hinting=true,xft:Noto Mono:pixelsize=20:antialias=true:hinting=true,xft:Symbola

URxvt.transparent:   true
URxvt.shading:       30
urxvt*termName:      rxvt

#Disable URxvt "picture mode"
URxvt.iso14755: true
URxvt.iso14755_52: false

URxvt.keysym.Shift-Control-V:eval:paste_clipboard
URxvt.keysym.Shift-Control-C:eval:selection_to_clipboard
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.scrollBar_floating: false
URxvt.scrollstyle: rxvt

Xft.dpi: 149
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

! special
*.foreground:   #7dfd81
*.background:   #000000
*.cursorColor:  #7dfd81

! black
*.color0:       #000000
*.color8:       #808080

! red
*.color1:       #ff0000
*.color9:       #ff0000

! green
*.color2:       #33ff00
*.color10:      #33ff00

! yellow
*.color3:       #ff0099
*.color11:      #ff0099

! blue
*.color4:       #0066ff
*.color12:      #0066ff

! magenta
*.color5:       #cc00ff
*.color13:      #cc00ff

! cyan
*.color6:       #00ffff
*.color14:      #00ffff

! white
*.color7:       #d0d0d0
*.color15:      #ffffff

